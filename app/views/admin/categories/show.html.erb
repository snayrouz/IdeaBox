<%= render partial: 'admin/categories/nav' %>

<div class="container-fluid">
  <table class="table table-striped text-left table-bordered">
  <h3><%= @category.name %> Category</h3>
    <thead>
      <tr>
        <th>User ID</th>
        <th>Idea</th>
        <th>Description</th>
      </tr>
        <tbody>
      <tr><% @category.ideas.each do |idea| %></tr>
          <td><%= idea.user_id %></td>
          <td><%= idea.title %></td>
          <td><%= idea.description %></td>
          <% end %>
      </tbody>
    </thead>
  </table>
<br>
</div>
